<!DOCTYPE html>
<html lang="en">
  <head>

  </head>
  <body>

    <main>
      <section class="container">
        <div class="item" id="contact">
          <h3>Contact</h3>
          <form action="" class="contact">
            <label for="name">
              Name:
              <input type="text" placeholder="Enter here" required />
            </label>
            <label for="usercontact">
              Email:
              <input type="text" placeholder="Enter here" id="" />
            </label>
            <label for="phone">
              Phone Number:
              <input type="text" placeholder="Enter here" />
            </label>
            <label for="textbox">
              <textarea
                name="textbox"
                id="textbox"
                cols="35"
                rows="10"
                placeholder="comments"
              ></textarea>
            </label>
            <input type="submit" value="submit" id="button" />
          </form>
        </div>
      </section>
    </main>

  </body>
</html>

